{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
        "email",
        "msg"
    ],
    "properties": {
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The Email Schema",
            "default": "",
            "examples": [
                "test@example.com"
            ],
            "pattern": "^(.*)$",
            "format": "email"
        },
        "msg": {
            "$id": "#/properties/msg",
            "type": "string",
            "title": "The Msg Schema",
            "default": "",
            "examples": [
                "hi there"
            ],
            "pattern": "^(.*)$"
        }
    }
}
